{
    "name": "drupal/la_eu",
    "description": "Drupal powered website for Local Associations",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/la_eu",
    "support": {
        "docs": "https://www.drupal.org/project/la_eu",
        "chat": "https://drupal.slack.com/archives/C05F15S2N2E"
    },
    "repositories": {
        "system_status": {
            "type": "package",
            "package": {
                "name": "drupal/system_status",
                "type": "drupal-module",
                "version": "2.x-dev@dev",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/project/system_status.git",
                    "reference": "8.x-2.x"
                }
            }
        },
        "_assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "_drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "bower-asset/lazysizes": "^5.3",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.0",
        "drupal/address_map_link": "^1.2",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advagg": "^6.0@alpha",
        "drupal/allowed_formats": "^2.0",
        "drupal/better_exposed_filters": "^6.0@beta",
        "drupal/block_field": "^1.0@alpha",
        "drupal/chosen": "^4.0",
        "drupal/ckeditorheight": "^1.0",
        "drupal/classy": "^1.0",
        "drupal/color": "^1.0",
        "drupal/components": "^3.0@beta",
        "drupal/config_split": "^1.9",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/csv_serialization": "^4.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/draggableviews": "^2.0",
        "drupal/embed": "^1.0",
        "drupal/entity": "^1.0",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/field_group": "^3.0",
        "drupal/field_hidden": "^2.0",
        "drupal/field_tools": "^1.0@alpha",
        "drupal/focal_point": "^2.0",
        "drupal/gin": "^3.0@alpha",
        "drupal/gtm": "^1.6",
        "drupal/http_cache_control": "^2.0",
        "drupal/imageapi_optimize": "~4.0.0@beta",
        "drupal/imageapi_optimize_gd": "^2.0@beta",
        "drupal/imageapi_optimize_tinypng": "^1.1@beta",
        "drupal/imageapi_optimize_webp": "^2.0@beta",
        "drupal/inline_block_title_automatic": "^1.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/layout_builder_browser": "^1.1",
        "drupal/layout_builder_iframe_modal": "^1.2",
        "drupal/layout_builder_limit": "^1.0@beta",
        "drupal/layout_builder_lock": "^1.1",
        "drupal/layout_builder_restrictions": "^2.7",
        "drupal/lazy": "^3.4",
        "drupal/lb_claro": "^2.0@alpha",
        "drupal/link_attributes": "^2.0",
        "drupal/linkit": "~6.0.0-beta4@beta",
        "drupal/login_security": "^2.0",
        "drupal/mailsystem": "^4.0",
        "drupal/markup": "^2",
        "drupal/matomo": "^1.19",
        "drupal/media_responsive_thumbnail": "^1.0",
        "drupal/menu_block": "^1.0",
        "drupal/metatag": "^2.0",
        "drupal/name": "^1.0",
        "drupal/node_revision_delete": "^1.0@RC",
        "drupal/paragraphs": "^1.0",
        "drupal/paragraphs_browser": "^1.0",
        "drupal/pathauto": "^1.0",
        "drupal/quicklink": "^2.0",
        "drupal/rdf": "^2.0",
        "drupal/redirect": "^1.0",
        "drupal/redis": "^1.5",
        "drupal/robotstxt": "^1.4",
        "drupal/scheduler": "^2.0",
        "drupal/scheduler_content_moderation_integration": "^2.0@beta",
        "drupal/seckit": "~2.0.0@RC",
        "drupal/security_review": "^2.0",
        "drupal/simple_sitemap": "^4.0",
        "drupal/site_settings": "^1.0",
        "drupal/sitemap": "^2.0@beta",
        "drupal/smart_trim": "^2.0",
        "drupal/svg_image_field": "^2.1",
        "drupal/svg_upload_sanitizer": "^1.0@alpha",
        "drupal/swiftmailer": "2.x-dev@dev",
        "drupal/system_status": "2.x-dev@dev",
        "drupal/twig_field_value": "~2.0.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/ultimate_cron": "^2.0",
        "drupal/username_enumeration_prevention": "^1.1",
        "drupal/views_bulk_edit": "^2.0",
        "drupal/views_bulk_operations": "^4.0",
        "drush/drush": "^11",
        "jjj/chosen": "^2.2",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "platformsh/config-reader": "^2.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Lemberg\\Draft\\Template\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "lemberg/draft-environment": true
        },
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "gitignore": false,
            "locations": {
                "web-root": "./docroot"
            },
            "file-mapping": {
               "[web-root]/robots.txt": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Field Groups marked as required are missing red asterisk": "https://www.drupal.org/files/issues/2020-09-18/required-asterisk-missing--3171835--2.patch",
                "Reverting entity revisions that contain custom blocks erroneously triggers EntityChangedConstraint": "https://www.drupal.org/files/issues/2021-04-06/3053881-3049332-45.patch"
            },
            "drupal/draggableviews": {
                "Not working with Group By feature": "https://www.drupal.org/files/issues/2021-10-11/not_working_with_group_by-draggableviews-2867348-48.patch"
            },
            "drupal/paragraphs": {
                "Affix correct langcode to paragraph in layout builder": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch",
                "Paragraphs do not render: access check for 'view' fail when using layout builder + custom blocks & content revisions because the incorrect parent revision ID is being passed in ParagraphAccessControlHandler::checkAccess(": "https://www.drupal.org/files/issues/2020-07-08/access-controll-issue-3090200-22.patch"
            },
            "drupal/ultimate_cron": {
                "[#3370283-3] PHP 8.2 / CronRule.php does not handle all variables properly / https://www.drupal.org/project/ultimate_cron/issues/3370283": "https://www.drupal.org/files/issues/2023-06-26/3370283.patch"
            },
            "drupal/views_bulk_edit": {
                "[#3024419] Fix Required fields to really not be required": "https://www.drupal.org/files/issues/2022-06-07/views_bulk_edit-fix_required_fields-3024419-29.patch"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Lemberg\\Draft\\Template\\DrupalProject::createRequiredFiles",
            "Lemberg\\Draft\\Template\\DrupalProject::localSetup"
        ],
        "post-update-cmd": [
            "Lemberg\\Draft\\Template\\DrupalProject::createRequiredFiles",
            "Lemberg\\Draft\\Template\\DrupalProject::localSetup"
        ]
    },
    "require-dev": {
        "drupal/devel": "^5.0",
        "drupal/stage_file_proxy": "^2.0",
        "lemberg/draft-environment": "^3.6"
    }
}
